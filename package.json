{
  "name": "io-player",
  "description": "IoPlayer - Audio Player web component",
  "version": "1.0.2",
  "license": "MIT",
  "keywords": [
    "Angular",
    "Custom",
    "Element",
    "Web",
    "Component",
    "AngularElements",
    "CustomElements",
    "WebComponents"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yanis-git/io-player.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod && gulp && ng-packagr -p package.json"
  },
  "private": false,
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "deleteDestPath": false,
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@angular",
      "@webcomponents",
      "core-js",
      "rxjs",
      "zone.js"
    ]
  },
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "@webcomponents/custom-elements": "^1.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
