{
  "name": "io-player",
  "description": "IoPlayer - Audio Player web component",
  "version": "1.0.5",
  "license": "MIT",
  "keywords": [
    "Angular",
    "Custom",
    "Element",
    "Web",
    "Component",
    "AngularElements",
    "CustomElements",
    "WebComponents"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yanis-git/io-player.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod && gulp && ng-packagr -p package.json",
    "publish:lib": "cd dist && npm publish && cd .."
  },
  "private": false,
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "deleteDestPath": false,
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@angular",
      "@webcomponents",
      "core-js",
      "rxjs",
      "zone.js"
    ]
  },
  "dependencies": {
    "@angular/animations": "^12.0.3",
    "@angular/common": "^12.0.3",
    "@angular/compiler": "^12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/elements": "^12.0.3",
    "@angular/forms": "^12.0.3",
    "@angular/platform-browser": "^12.0.3",
    "@angular/platform-browser-dynamic": "^12.0.3",
    "@angular/router": "^12.0.3",
    "@webcomponents/custom-elements": "^1.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.6.7",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.3",
    "@angular/cli": "^12.0.3",
    "@angular/compiler-cli": "^12.0.3",
    "@types/jasmine": "^3.6.10",
    "@types/node": "^12.20.11",
    "codelyzer": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^2.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.2.3"
  }
}
