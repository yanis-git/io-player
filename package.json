{
  "name": "io-player",
  "description": "IoPlayer - Audio Player web component",
  "version": "1.0.2",
  "license": "MIT",
  "keywords": [
    "Angular",
    "Custom",
    "Element",
    "Web",
    "Component",
    "AngularElements",
    "CustomElements",
    "WebComponents"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yanis-git/io-player.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build --prod && gulp && ng-packagr -p package.json"
  },
  "private": false,
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "deleteDestPath": false,
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "@angular",
      "@webcomponents",
      "core-js",
      "rxjs",
      "zone.js"
    ]
  },
  "peerDependencies": {
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.24",
    "@webcomponents/custom-elements": "^1.1.0"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/elements": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@webcomponents/custom-elements": "^1.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.3",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.9.4",
    "codelyzer": "^4.2.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.5",
    "protractor": "^5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
